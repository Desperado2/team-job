<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.desperado.teamjob.dao.LoginRecordDao">

    <insert id="addRecord" parameterType="com.desperado.teamjob.domain.LoginRecord">
        INSERT INTO `login_record`(
                username,ip_address,browser_type,address,login_time
        ) VALUES (#{username},#{ipAddress},#{browserType},#{address},#{loginTime})
    </insert>



</mapper>